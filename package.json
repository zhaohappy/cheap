{
  "name": "@libmedia/cheap",
  "version": "1.1.0",
  "description": "一个多线程和高性能 wasm 开发库",
  "sideEffects": [
    "./src/heap.ts",
    "./src/thread/initClass.ts",
    "./src/thread/initFunction.ts",
    "./src/thread/initModule.ts",
    "./src/webassembly/runThread.ts",
    "./dist/esm/heap.js",
    "./dist/esm/thread/initClass.js",
    "./dist/esm/thread/initFunction.js",
    "./dist/esm/thread/initModule.js",
    "./dist/esm/webassembly/runThread.js",
    "./dist/mjs/heap.js",
    "./dist/mjs/thread/initClass.js",
    "./dist/mjs/thread/initFunction.js",
    "./dist/mjs/thread/initModule.js",
    "./dist/mjs/webassembly/runThread.js",
    "./dist/cjs/heap.js",
    "./dist/cjs/thread/initClass.js",
    "./dist/cjs/thread/initFunction.js",
    "./dist/cjs/thread/initModule.js",
    "./dist/cjs/webassembly/runThread.js",
    "./dist/cheap-polyfill.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaohappy/cheap.git"
  },
  "scripts": {
    "build-transformer": "rollup -c --environment transformer:1",
    "build-wasm-opt": "rollup -c --environment wasm_opt:1",
    "test-transformer": "jest --verbose",
    "prepublishOnly": "node ./scripts/check-publish-status.js"
  },
  "files": [
    "dist",
    "include",
    "build",
    "src"
  ],
  "author": "Gaoxing Zhao",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "cross-env": "^7.0.3",
    "jest": "^30.2.0",
    "rollup": "^4.53.2",
    "ts-jest": "^29.4.5",
    "ts-jest-mock-import-meta": "^1.3.1",
    "typescript": "^5.9.3",
    "yargs": "^13.3.0"
  },
  "dependencies": {
    "@libmedia/common": "workspace:*",
    "commander": "^13.1.0"
  },
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/mjs/index.js",
  "browser": "./dist/esm/index.js",
  "exports": {
    ".": {
      "browser": "./dist/esm/index.js",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/mjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./internal": {
      "browser": "./dist/esm/internal.js",
      "import": "./dist/mjs/internal.js",
      "require": "./dist/cjs/internal.cjs",
      "default": "./dist/mjs/internal.js",
      "types": "./dist/esm/internal.d.ts"
    },
    "./cheapdef": {
      "browser": "./dist/esm/cheapdef.js",
      "import": "./dist/mjs/cheapdef.js",
      "require": "./dist/cjs/cheapdef.cjs",
      "default": "./dist/mjs/cheapdef.js",
      "types": "./dist/esm/cheapdef.d.ts"
    },
    "./runThread": {
      "browser": "./dist/esm/thread/runThread.js",
      "import": "./dist/mjs/thread/runThread.js",
      "require": "./dist/cjs/thread/runThread.cjs",
      "default": "./dist/mjs/thread/runThread.js",
      "types": "./dist/esm/thread/runThread.d.ts"
    },
    "./build/CheapPlugin": "./build/webpack/plugin/CheapPlugin.js",
    "./build/transformer": {
      "import": "./build/transformer.mjs",
      "require": "./build/transformer.cjs",
      "default": "./build/transformer.mjs",
      "types": "./build/transformer.d.ts"
    },
    "./build/wasm-opt": {
      "require": "./build/wasm-opt.cjs",
      "import": "./build/wasm-opt.mjs",
      "default": "./build/wasm-opt.mjs"
    }
  }
}